<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/ejb-jar_3_2.xsd"
	version="3.2">
	<display-name>JavaTunesEjb</display-name>
	<ejb-client-jar>JavaTunesEjbClient.jar</ejb-client-jar>

	<enterprise-beans>
		<session>
			<ejb-name>SearchUtilityBean</ejb-name>
			<env-entry>
				<env-entry-name>maxSearchResults</env-entry-name>
				<env-entry-type>java.lang.Integer</env-entry-type>
				<env-entry-value>6</env-entry-value>
			</env-entry>
		</session>
	</enterprise-beans>
	
</ejb-jar>